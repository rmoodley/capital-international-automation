        <InstructionsMainLabel>
            <identifier>xpath</identifier>
            <Element>//*[contains(./text(),'Instructions')]</Element>
        </InstructionsMainLabel>
        <InstructionsMainIcon>
            <identifier>xpath</identifier>
            <Element>//img[contains(@src, 'instructions.svg')]</Element>
        </InstructionsMainIcon>
        <FXLabel>
            <identifier>xpath</identifier>
            <Element>//*[contains(./text(),'FX')]</Element>
